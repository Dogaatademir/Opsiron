{
  "name": "opsiron-monorepo",
  "version": "1.0.0",
  "description": "opsiron monorepo with CraftOps applications",
  "private": true,
  "scripts": {
    "install:all": "cd apps/opsiron-web && npm install && cd ../craftops/editioncoffeeroastery && npm install",
    "build:opsiron": "cd apps/opsiron-web && npm run build",
    "build:editioncoffee": "cd apps/craftops/editioncoffeeroastery && npm run build",
    "build:all": "npm run build:opsiron && npm run build:editioncoffee",
    "dev:opsiron": "cd apps/opsiron-web && npm run dev",
    "dev:editioncoffee": "cd apps/craftops/editioncoffeeroastery && npm run dev"
  },
  "keywords": [
    "opsiron",
    "craftops",
    "monorepo"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "react-helmet-async": "^2.0.5"
  }
}
